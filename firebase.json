{
  "hosting": {
    "public": "out",
    "target": "website",
    "ignore": [
      ".firebaserc",
      "database.rules.json",
      "firebase.json",
      "package.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "trailingSlash": true,
    "headers": [
      {
        "source": "/**",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' https://app.mockoon.com;"
          },
          {
            "key": "Referrer-Policy",
            "value": "strict-origin-when-cross-origin"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin-allow-popups"
          }
        ]
      },
      {
        "source": "/desktop-changelogs-markdown/**",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type, Origin, Accept, Authorization, Content-Length, X-Requested-With, Cache-Control, pragma"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/discussions{,/}",
        "destination": "https://github.com/mockoon/mockoon/discussions",
        "type": 301
      },
      {
        "source": "/enterprise{,/}",
        "destination": "/pro/",
        "type": 301
      },
      {
        "source": "/pro{,/}",
        "destination": "/cloud/",
        "type": 301
      },
      {
        "source": "/course{,/}",
        "destination": "/training/",
        "type": 301
      },
      {
        "source": "/account{,/}",
        "destination": "/account/info/",
        "type": 301
      },
      {
        "source": "/releases{,/}",
        "destination": "/releases/9.3.0/",
        "type": 302
      },
      {
        "source": "/releases/desktop{,/**}",
        "destination": "/old-releases/desktop/",
        "type": 301
      },
      {
        "source": "/releases/cli{,/**}",
        "destination": "/old-releases/cli/",
        "type": 301
      },
      {
        "source": "/releases/serverless{,/**}",
        "destination": "/old-releases/serverless/",
        "type": 301
      },
      {
        "source": "/docs{,/}",
        "destination": "/docs/latest/about/",
        "type": 301
      },
      {
        "source": "/docs/latest{,/}",
        "destination": "/docs/latest/about/",
        "type": 301
      },
      {
